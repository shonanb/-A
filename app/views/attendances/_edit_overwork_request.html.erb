<div class="modal-dialog model-lg modal-dialog-center">
  <div class="modal-content">
    <div class="modal-header">
      <button type="button" class="close" data-dismiss="modal" aria-label="Close">
        <span aria-hidden="true">&times;</span>
      </button>
      <h1 class="modal-title">残業申請</h1>
    </div>
    <div class="modal-body">
          <%= form_with(model: @attendance, url:update_overwork_request_user_attendance_path(@user, @attendance), local: true, method: :patch) do |f| %>

            <table class="table">
              <thead>
                <tr>
                  <th>日付</th>
                  <th>曜日</th>
                  <th>終了予定時間</th>
                  <th>翌日</th>
                  <th>業務処理内容</th>
                  <th>指示者確認㊞</th>
                </tr>
              </thead>
              <tbody>
                <tr>
                  
                </tr>
              </tbody>
            </table>
              <%= f.submit "変更を送信する", class: "btn btn-primary btn-block" %>
            <% end %>
        </div>
      </div>
    </div>